function plot_ {
	xx=x; yy=y
	if [[ $rsi -ge 64 ]] && [[ $rsi -le 69 ]]; then
		varname=x14yn10
	elif [[ $rsi -ge 55 ]] && [[ $rsi -le 63 ]]; then
		varname=x10yn8
	elif [[ $rsi -ge 45 ]] && [[ $rsi -le 54 ]]; then
		varname=xn3yn3
	elif [[ $rsi -ge  44 ]] && [[ $rsi -le 40 ]]; then
		varname=x10y3
	elif [[ $rsi -ge 30 ]] && [[ $rsi -le 39 ]]; then
		varname=x12y3
	elif [[ $rsi -lt 29 ]]; then
		varname=x22y3
	fi
	eval ${varname}="*"
}
while true; do
	rsi=$(/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport /usr/sbin/airport -I | grep "agrCtlRSSI:" | awk '{print $2}' | sed 's/[-]//g')
	clear_
	plot_
	disp_
	sleep 0.3
done